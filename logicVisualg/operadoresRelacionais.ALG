algoritmo "operadoresRelacionais"
var
   a, b, c: inteiro
inicio
      // > maior, < menor, >= maior ou igual, <= menor ou igual, = igual, <> != diferente
      a <- 2
      b <- 3
      c <- 5
      escreval(a>b)
      escreval(a>=2)
      escreval(c=a+b)
      escreva(c<=b^a)
fimalgoritmo