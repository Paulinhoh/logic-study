algoritmo "operadoresLogicos"
var
   a,b,c: inteiro
inicio
      // E somente quando os dois forem verdadeiros
      // Ou quando pelo menos um é verdadeiro
      // Nao inverte o resultado
      a <- 2
      b <- 3
      c <- 5
      escreva(nao((a=b)ou(c>a)))
      // Aritimeticos, Relacionais, Lógicos
fimalgoritmo